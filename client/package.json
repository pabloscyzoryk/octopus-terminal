{
  "name": "octopus-client",
  "version": "1.0.0",
  "description": "client for octopus terminal software",
  "main": "index.js",
  "bin": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js --no-experimental-fetch",
    "build": "babel src --extensions \".ts\" --out-dir dist --watch",
    "dev": "nodemon --quiet ./dist/index.js --no-experimental-fetch",
    "make": "pkg -o ./package/octopus.exe .",
    "debug": "pkg --debug --log=verbose -o ./package/octopus.exe .",
    "octopus": "start ./package/octopus.exe"
  },
  "keywords": [
    "ts",
    "typescript",
    "cmd",
    "terminal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabloscyzoryk/octopus-terminal.git"
  },
  "author": "pabloscyzoryk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pabloscyzoryk/octopus-terminal/issues"
  },
  "homepage": "https://github.com/pabloscyzoryk/octopus-terminal#readme",
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/figlet/fonts/Standard.flf",
      ".env"
    ],
    "targets": [
      "node18-win"
    ],
    "outputPath": "./package/"
  },
  "dependencies": {
    "@types/node": "^20.10.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-typescript": "^7.23.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-minify": "^0.5.2"
  }
}
